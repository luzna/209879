class Apple extends Fruit
{
    private string cultivar;
    private bool ifWormy;

    public getCultivar()
    {
	return cultivar;
    }
    public getIfWormy()
    {
	return ifWormy;
    }
    public setCultivar(string cultivar)
    {
	this.cultivar = cultivar;
    }
    public setIfWormy(bool ifWormy)
    {
	this.ifWormy = ifWormy;
    }

    public Apple() : Fruit()
    {
	cultivar = "unknown";
	ifWormy = false;
	count++;
    }
    public Apple(Apple apple) : Fruit(apple)
    {
	this.cultivar = cultivar;
	this.ifWormy = ifWormy;
	count++;
    }
    private void finalize()
    {
	count--;
    }
}